<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Symfony\Cmf\Bundle\RoutingBundle\Doctrine\Orm\RedirectRoute" table="orm_redirect_routes">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="uri" type="string"/>
        <field name="routeName" type="string"/>
        <field name="permanent" type="boolean"/>
        <field name="parameters" type="array"/>
    </entity>

</doctrine-mapping>
